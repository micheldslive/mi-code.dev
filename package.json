{
  "name": "micode.dev",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write --plugin-search-dir=. **/*.{cjs,mjs,ts,tsx}",
    "format:check": "prettier --check *.{cjs,md,json} --ignore-unknown --no-error-on-unmatched-pattern",
    "check": "pnpm lint && pnpm format:check && pnpm type-check",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit",
    "test": "jest",
    "test:watch": "pnpm test --watch",
    "prettier:check": "prettier --check \"src/**/*.{ts,tsx}\"",
    "prettier:format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "prepare": "husky install",
    "storybook": "storybook dev -p 6006 -s ../public",
    "build-storybook": "storybook build -s public",
    "generate": "pnpm plop --plopfile .generate/plopfile.js"
  },
